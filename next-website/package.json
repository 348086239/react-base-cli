{
  "name": "my-next-app",
  "private": true,
  "main": "index.tsx",
  "scripts": {
    "dev": "cross-env CI=false REACT_APP_ENV=dev REACT_APP_URL=''  REACT_APP_API_URL='' next",
    "qa": "cross-env CI=false REACT_APP_ENV=qa REACT_APP_URL=''  REACT_APP_API_URL='' next start",
    "pre": "cross-env CI=false REACT_APP_ENV=pre REACT_APP_URL=''  REACT_APP_API_URL='' next start",
    "prod": "cross-env CI=false REACT_APP_ENV=prod REACT_APP_URL=''  REACT_APP_API_URL='' next start",
    "build:qa": "cross-env CI=false REACT_APP_ENV=qa REACT_APP_URL=''  REACT_APP_API_URL=''  next build",
    "build:pre": "cross-env CI=false REACT_APP_ENV=pre REACT_APP_URL=''  REACT_APP_API_URL='' next build",
    "build:prod": "cross-env CI=false REACT_APP_ENV=prod REACT_APP_URL=''  REACT_APP_API_URL='' next build",
    "postbuild": "cross-env CI=false next-sitemap --config next-sitemap.config.js next build",
    "prepare": "husky install",
    "lint": "next lint",
    "lint-staged:js": "eslint --ignore-path .eslintignore --fix --ext .js,.jsx,.ts,.tsx ./pages ./components",
    "lint:fix": "eslint --ignore-path .eslintignore --fix --ext .js,.jsx,.ts,.tsx ./pages ./components"
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts,scss,json}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "npm run lint-staged:js",
      "git add"
    ],
    "*.js": [
      "eslint --cache --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.25%",
      "not dead",
      "not op_mini all",
      "ie >= 9"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie >= 9"
    ]
  },
