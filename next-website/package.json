{
  "name": "my-app",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "cross-env CI=false REACT_APP_ENV=dev REACT_APP_URL=''  REACT_APP_API_URL='' next",
    "pre": "cross-env CI=false REACT_APP_ENV=pre REACT_APP_URL=''  REACT_APP_API_URL='' next",
    "qa": "cross-env CI=false REACT_APP_ENV=qa REACT_APP_URL=''  REACT_APP_API_URL='' next",
    "build:pre": "cross-env CI=false REACT_APP_ENV=pre REACT_APP_URL=''  REACT_APP_API_URL='' next build",
    "build:qa": "cross-env CI=false REACT_APP_ENV=qa REACT_APP_URL=''  REACT_APP_API_URL='' next build",
    "build:prod": "cross-env CI=false REACT_APP_ENV=prod REACT_APP_URL=''  REACT_APP_API_URL='' next build && npm run postbuild",
    "postbuild": "cross-env CI=false next-sitemap --config next-sitemap.config.js",
    "lint": "next lint",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx ./src",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts,scss,json}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "npm run lint-staged:js",
      "git add"
    ],
    "*.js": [
      "eslint --cache --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie >= 9"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie >= 9"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "@zeit/next-css": "^1.0.1",
    "ahooks": "^3.3.10",
    "antd": "^4.20.3",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "js-base64": "^3.7.2",
    "js-cookie": "^3.0.1",
    "localStorage": "^1.0.4",
    "lodash": "^4.17.21",
    "next": "12.1.6",
    "next-redux-wrapper": "^7.0.5",
    "prismjs": "^1.28.0",
    "qrcode.react": "^3.0.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-redux": "^8.0.1",
    "react-router": "^5.3.1",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@styled-jsx/plugin-sass": "^4.0.2",
    "@types/axios": "^0.14.0",
    "@types/crypto-js": "^4.1.1",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "17.0.31",
    "@types/prismjs": "^1.26.0",
    "@types/react": "18.0.9",
    "@types/react-router": "^5.1.18",
    "@types/styled-jsx": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "babel-plugin-import": "^1.13.5",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.0",
    "less": "^4.1.2",
    "less-loader": "^6.2.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^12.4.1",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.8.4",
    "next-plugin-antd-less": "^1.8.0",
    "next-sitemap": "^2.5.20",
    "next-transpile-modules": "^9.0.0",
    "prettier": "^2.6.2",
    "redux-devtools-extension": "^2.13.9",
    "sass": "^1.51.0",
    "typescript": "4.6.4",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
