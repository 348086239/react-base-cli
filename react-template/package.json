  "main": "index.js",
  "scripts": {
    "build:pre": "dotenv -e .env.pre cross-env REACT_APP_ENV=pre REACT_APP_NODE_CDN_ENV=pre CI=false react-app-rewired build",
    "build:prod": "dotenv -e .env.prod cross-env  REACT_APP_ENV=prod REACT_APP_NODE_CDN_ENV=prod CI=false react-app-rewired build",
    "build:qa": "dotenv -e .env.qa cross-env REACT_APP_ENV=qa REACT_APP_NODE_CDN_ENV=qa CI=false react-app-rewired build",
    "dev": "dotenv -e .env.dev cross-env  REACT_APP_ENV=dev REACT_APP_NODE_CDN_ENV=dev CI=false react-app-rewired start",
    "pre": "dotenv -e .env.pre cross-env  REACT_APP_ENV=pre REACT_APP_NODE_CDN_ENV=pre CI=false react-app-rewired start",
    "qa": "dotenv -e .env.qa cross-env  REACT_APP_ENV=qa REACT_APP_NODE_CDN_ENV=qa CI=false  react-app-rewired start",
    "release": "node ./node_modules/standard2version/lib/release.js",
    "start": "dotenv -e .env.dev cross-env  REACT_APP_ENV=dev REACT_APP_NODE_CDN_ENV=dev CI=false react-app-rewired start",
    "style-lint": "stylelint \"src/**/*.(scss|css)\" --fix",
    "lint-staged:js": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "test": "react-app-rewired test"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint-staged:js",
      "git add"
    ],
    "*.{scss,css}": [
      "npm run style-lint",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie >= 9"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie >= 9"
    ]
  },
